{
  "version": 2,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404/"
    }
  ],
  "redirects": [
    {
      "source": "/writing",
      "destination": "/"
    },
    {
      "source": "/projects",
      "destination": "/"
    }
  ]
}
